
<header role="banner" class="container-fluid">
  <div class="row py-3">
    <div class="col-lg-4 col-xl-3 offset-xl-1">
      {% include 'back-btn' %}
    </div>    
  </div>
</header>
<div id="music-unity">
  <div class="mb-5">
    <h1 class="text-center">{{ page.title }}</h1>
    {% if page.content %}
      <div class="container mt-3">
        <div class="row">
          <div class="col">
            {{ page.content }}
          </div>
        </div>
      </div>
    {% endif %}
  </div>

  {% section 'page-music_featured' %}

  {% section 'page-music_latest-releases' %}
</div>

{% include 'back-to-top-dark' %}
